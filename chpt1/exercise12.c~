#include <stdio.h>
#define IN 1
#define OUT 0

int main() {
  int c, state;
  c = getchar();
  state = OUT;
  while (c != EOF) {
    if (c == ' ' || c == '\n' || c == 't') {
      if (state == OUT) {
        printf("\n");
      }
      state = OUT;
    } else {
      state = IN;
    }
    c = getchar();
  }
}
